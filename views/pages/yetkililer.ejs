<main class="container mt-4">
  <div class="row justify-content-center">
    <% membersWithRole.forEach(member => { %>
      <div class="col-sm-6 col-md-4 col-lg-3 mb-4">
        <div class="card text-center bg-dark">
          <div class="rounded-circle mx-auto mt-3" style="width: 75px; height: 75px; overflow: hidden;">
            <img src="<%= member.avatar %>" alt="<%= member.username %>" style="width: 100%;">
          </div>
          <div class="card-body">
            <h5 class="card-title"><%= member.username %></h5>
          </br>
            <h5 class="card-title text-info">Owner</h5>

          </div>
        </div>
      </div>
    <% }); %>
  </div>
</main>


